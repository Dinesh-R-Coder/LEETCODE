class Solution {
    public int[][] transpose(int[][] matrix) {
        int m = matrix.length , n = matrix[0].length ;
        int[][] transpose = new int[n][m] ;
        for(int r = 0 ; r < m ; r++){
             for(int c = 0 ; c < n ; c++){
                   transpose[c][r] = matrix[r][c] ;
             }
        }
        return transpose ;
    }
}
